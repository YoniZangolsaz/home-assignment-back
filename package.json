{
  "name": "template-express",
  "version": "1.0.0",
  "description": "template-express",
  "main": ".prettierrc.js",
  "scripts": {
    "start": "nodemon ./src/index.ts",
    "dev": "npm run build && npm start",
    "test": "jest --coverage",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prettier": "prettier --config .prettierrc.js 'src/**/*.ts' --check",
    "prettier:fix": "prettier --config .prettierrc.js 'src/**/*.ts' --write",
    "clean": "rimraf dist",
    "docker-build": "docker build -t template-express .",
    "docker-compose": "docker-compose up -d"
  },
  "author": "Alexander Gurevich",
  "license": "ISC",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/770menahem/tamplate-express.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/770menahem/tamplate-express/issues"
  },
  "homepage": "https://github.com/770menahem/tamplate-express#readme",
  "dependencies": {
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "env-var": "^7.1.1",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.4.0",
    "morgan": "^1.10.0",
    "supertest": "^6.2.3",
    "winston": "^3.8.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.0",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "jest": "^28.1.1",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  }
}
